        padding-right:25px;
        padding-left:25px;
        padding-bottom: 10px;
    }
   #home #selectionHome{
        border-top:2px solid black;
    }

    @keyframes hueme {
        0% {filter: hue-rotate(0deg);}
        12% {filter: hue-rotate(90deg);}
        25% {filter: hue-rotate(180deg);}
        37% {filter: hue-rotate(270deg);}
        50% {filter: hue-rotate(360deg);}
        100% {filter: hue-rotate(360deg);}
    }
