        height:540px;
        width:100%;
        background-position: 50% 60%;
        background-repeat: no-repeat;
        transition:all 0.2s linear;
        cursor: pointer;
    }

    #home .departmentSelector .WOMEN{
        background-image: url('/images/home/home_women.jpg');
        animation-name: hueme;
        animation-duration: 8s;
        animation-iteration-count: infinite;

    }
    #home .departmentSelector .MEN{
        background-image: url('/images/home/home_men.jpg');
