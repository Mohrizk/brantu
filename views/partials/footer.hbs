                {{#if (compareStrings this.department.name ../selectedDepartment)}}
                <div class="text-center col-md-4 col-sm-4 col-xs-6  no-overflow-text">
                    <h5 class=" text-uppercase text-spaced text-black p-b-10 sbold">Kategorier </h5>
                    <ul class="no-style no-margin no-padding">
                        {{#each categories}}
                        <li class="m-b-5   m-xs-b-0  no-padding "><a class="dark" data-department-explore = "{{this.department.name}}"  href="{{category.url}}">{{andfixer this.category.name}}</a></li>
                        {{/each}}
                    </ul>
                </div>
                    {{/if}}
            {{/each}}

            {{#if (compareStrings 'kvinna' selectedDepartment)}}
                <div class="text-center col-md-4 col-sm-4 col-xs-6">
